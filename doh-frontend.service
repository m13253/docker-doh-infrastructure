[Unit]
Description=DoH Frontend
After=docker.service

[Service]
ExecStartPre=-/usr/bin/env docker run -d -p [::1]:18053:8053 --name doh-frontend --rm doh-infrastructure:frontend
ExecStart=/usr/bin/env docker start -a doh-frontend
ExecStop=/usr/bin/env docker stop doh-frontend
Type=simple

[Install]
WantedBy=multi-user.target
